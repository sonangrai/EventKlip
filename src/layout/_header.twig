<!-- Login modal -->
<div
  class="modal fade"
  id="loginModal"
  tabindex="-1"
  aria-labelledby="exampleModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog login-modal">
    <div class="modal-content contents">
      <div class="header">
        <div class="label">
          <strong>Log In</strong>
        </div>
        <div class="action">
          <ul>
            <li>
              <a
                href="#!"
                data-bs-toggle="modal"
                data-bs-target="#signupModal"
                data-bs-dismiss="modal"
                >Sign Up</a
              >
            </li>
            <li>
              <a href="#!" data-bs-dismiss="modal">Close</a>
            </li>
          </ul>
        </div>
      </div>
      <div class="body">
        <div class="content">
          <div class="illustration">
            <img src="./img/login-illus.png" alt="" />
          </div>
          <form>
            <div class="form-item">
              <input
                type="text"
                class="form-control"
                id="phone"
                placeholder="Mobile Number"
              />
            </div>
            <div class="form-item">
              <input
                type="password"
                class="form-control"
                id="password"
                placeholder="Password"
              />
              <span class="eye">
                <svg
                  width="24"
                  height="24"
                  class="show-eye"
