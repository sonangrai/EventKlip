{% extends '../layout/_layout-dashboard.twig' %} {% block title %} Sam & Sammy
Wedding - EventKlip {% endblock %} {% block javascripts %} {% endblock %} {%
block contents %}
<main class="dashboard--main">
  <!-- BreadCrumb -->
  <div class="topbar">
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="./">Dashboard</a></li>
        <li class="breadcrumb-item active" aria-current="page">
          Sam & Sammy Wedding
        </li>
      </ol>
    </nav>
  </div>

  <!-- Event Detail -->
  <section class="contents single-content">
    <div class="event-row">
      <div class="event-info">
        <div class="hero-image">
          <img
            src="https://cdn.pixabay.com/photo/2016/09/04/07/40/couple-1643452__340.jpg"
            alt=""
          />
        </div>

        <div class="the-descriptions">
          <p>Hello <strong>Username</strong></p>
          <p>Welcome to Sam’s & Sammy’s Wedding. Enjoy the event.</p>
          <p>Start clicking pictures and videos of the event.</p>

          <p>Enjoy!</p>
        </div>
      </div>

      <div class="event-actions">
        <div class="title">
          <h4>Event By</h4>
        </div>
        <div class="prev">
          <img
            src="https://cdn.pixabay.com/photo/2014/11/13/17/04/heart-529607__340.jpg"
            alt=""
          />
        </div>
        <a href="./gallery" class="btn btn-main"
          ><i class="bi bi-images"></i> Gallery</a
        >
        <a href="#" class="btn btn-main">Enter</a>
      </div>
    </div>
  </section>
</main>

{% endblock %}
