{% extends '../layout/_layout-dashboard.twig' %} {% block title %} Sam & Sammy
Wedding - EventKlip Gallery {% endblock %} {% block stylesheets %}
<link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/magnific-popup@1.1.0/dist/magnific-popup.css"
/>
{% endblock %} {% block javascripts %} {% endblock %} {% block contents %}
<main class="dashboard--main">
  <!-- BreadCrumb -->
  <div class="topbar">
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="./">Dashboard</a></li>
        <li class="breadcrumb-item">
          <a href="./event">Sam & Sammy Wedding </a>
        </li>
        <li class="breadcrumb-item active" aria-current="page">
          Sam & Sammy Wedding Gallery
        </li>
      </ol>
    </nav>
  </div>

  <!-- Event Detail -->
  <section class="contents single-content">
    <div class="gallery-contents">
      <ul class="nav nav-tabs gallery-tabs" id="myTab" role="tablist">
        <li class="nav-item" role="presentation">
          <button
            class="nav-link active"
            id="all-tab"
            data-bs-toggle="tab"
            data-bs-target="#all"
            type="button"
            role="tab"
            aria-controls="all"
            aria-selected="true"
          >
            All <span class="count">(14)</span>
          </button>
        </li>
        <li class="nav-item" role="presentation">
          <button
            class="nav-link"
            id="photos-tab"
            data-bs-toggle="tab"
            data-bs-target="#photos"
            type="button"
            role="tab"
            aria-controls="photos"
            aria-selected="false"
          >
            Photos
          </button>
        </li>
        <li class="nav-item" role="presentation">
          <button
            class="nav-link"
            id="videos-tab"
            data-bs-toggle="tab"
            data-bs-target="#videos"
            type="button"
            role="tab"
            aria-controls="videos"
            aria-selected="false"
          >
            Videos
          </button>
        </li>
      </ul>
      <div class="tab-content gallery-content" id="myTabContent">
        <div
          class="tab-pane fade show active"
          id="all"
          role="tabpanel"
          aria-labelledby="all-tab"
        >
          <!-- The Group of Gallery Items by Day -->
          <div class="pack">
            <div class="date-title">
              <h3>Sat, April 17</h3>
            </div>
            <div class="gal-rows">
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2015/03/30/12/35/sunset-698501__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2015/03/30/12/35/sunset-698501__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2016/04/26/22/31/bride-1355473__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2016/04/26/22/31/bride-1355473__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2017/08/06/20/11/wedding-2595862__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2017/08/06/20/11/wedding-2595862__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2017/09/25/09/35/wedding-2784455__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2017/09/25/09/35/wedding-2784455__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2016/11/22/19/05/couple-1850073__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2016/11/22/19/05/couple-1850073__340.jpg"
                    alt=""
                  />
                </a>
              </div>
            </div>
          </div>

          <div class="pack">
            <div class="date-title">
              <h3>Sat, April 16</h3>
            </div>
            <div class="gal-rows">
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2016/06/29/04/17/wedding-dress-1485984__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2016/06/29/04/17/wedding-dress-1485984__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item type-video">
                <a href="https://youtu.be/b4iOwqZCmv4">
                  <img
                    src="https://cdn.pixabay.com/photo/2016/11/18/18/32/wedding-1836315__340.jpg"
                    alt=""
                  />
                </a>
                <i class="bi bi-play-circle-fill"></i>
              </div>
            </div>
          </div>
        </div>
        <div
          class="tab-pane fade"
          id="photos"
          role="tabpanel"
          aria-labelledby="photos-tab"
        >
          <!-- The Group of Gallery Items by Day -->
          <div class="pack">
            <div class="date-title">
              <h3>Sat, April 17</h3>
            </div>
            <div class="gal-rows">
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2015/03/30/12/35/sunset-698501__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2015/03/30/12/35/sunset-698501__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2016/04/26/22/31/bride-1355473__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2016/04/26/22/31/bride-1355473__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2017/08/06/20/11/wedding-2595862__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2017/08/06/20/11/wedding-2595862__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2017/09/25/09/35/wedding-2784455__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2017/09/25/09/35/wedding-2784455__340.jpg"
                    alt=""
                /></a>
              </div>
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2016/11/22/19/05/couple-1850073__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2016/11/22/19/05/couple-1850073__340.jpg"
                    alt=""
                  />
                </a>
              </div>
            </div>
          </div>

          <div class="pack">
            <div class="date-title">
              <h3>Sat, April 16</h3>
            </div>
            <div class="gal-rows">
              <div class="item">
                <a
                  href="https://cdn.pixabay.com/photo/2016/06/29/04/17/wedding-dress-1485984__340.jpg"
                >
                  <img
                    src="https://cdn.pixabay.com/photo/2016/06/29/04/17/wedding-dress-1485984__340.jpg"
                    alt=""
                /></a>
              </div>
            </div>
          </div>
        </div>
        <div
          class="tab-pane fade"
          id="videos"
          role="tabpanel"
          aria-labelledby="videos-tab"
        >
          <div class="pack">
            <div class="date-title">
              <h3>Sat, April 16</h3>
            </div>

            <div class="gal-rows">
              <div class="item type-video">
                <a href="https://youtu.be/b4iOwqZCmv4">
                  <img
                    src="https://cdn.pixabay.com/photo/2016/11/18/18/32/wedding-1836315__340.jpg"
                    alt=""
                  />
                </a>
                <i class="bi bi-play-circle-fill"></i>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</main>

<!-- Popup JS-->
<script
  src="https://code.jquery.com/jquery-3.6.0.min.js"
  integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
  crossorigin="anonymous"
></script>
<script src="https://cdn.jsdelivr.net/npm/magnific-popup@1.1.0/dist/jquery.magnific-popup.min.js"></script>
<script>
  $(document).ready(function () {
    $(".gal-rows .item a").magnificPopup({
      type: "image",
      gallery: {
        enabled: true,
      },
      zoom: {
        enabled: true,
        duration: 300,
        easing: "ease-in-out",
      },
    });
  });
</script>

{% endblock %}
